.row-fluid
  .span3.searches
    %h1 Search
    %input.search


:css
  .searches{
    height:100%;
  }
  .ui-autocomplete{
    width:350px;
    height:500px;
    overflow-y:scroll;
    overflow-x:hidden;
  }

  .search{
    width:352px;
  }


:javascript
  $('.search').autocomplete({
    delay: 50,
    minLength: 3,
    source: #{@names}
  });

